<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="kr.co.yooncheolcommunity.api.user.dao.UserDao">

    <!-- 사용자 조회 쿼리 -->
    <select id="findByUserId" parameterType="String" resultType="kr.co.yooncheolcommunity.api.model.User">
        SELECT * FROM user WHERE user_id = #{userId}
    </select>

    <!-- 사용자 추가 쿼리 -->
    <insert id="insertUser" parameterType="kr.co.yooncheolcommunity.api.model.User">
        INSERT INTO user (user_id, password) VALUES (#{userId}, #{password})
    </insert>

</mapper>